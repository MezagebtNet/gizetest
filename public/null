  	$within_days = $rent->rental_detail->within_days;
  	
  	$end_date = \Carbon\Carbon::createFromFormat('Y-m-d H:i:s', $start_date)->addDays($within_days);
	
  
	$check = $now->between($start_date, $end_date);   
    if($check){
				
	  	$rentals = $rentals->merge($rent);
	  
	}
}

 $rentals;

